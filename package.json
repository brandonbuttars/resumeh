{
  "name": "resumeh",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonbuttars/resumeh.git"
  },
  "main": "index.js",
  "scripts": {
    "bs": "browser-sync start -c ./config/bs-config.js",
    "build": "run-s rollup scss",
    "build:dist": "run-s rollup:dist scss:dist",
    "build:watch": "run-p rollup:watch scss:watch",
    "rollup": "node ./build/rollup.js",
    "rollup:cjs": "npm run rollup -- cjs",
    "rollup:es": "npm run rollup -- es",
    "rollup:iife": "npm run rollup -- iife",
    "rollup:dist": "run-s rollup rollup:cjs rollup:es rollup:iife",
    "rollup:watch": "npm run rollup -- watch",
    "scss": "node ./build/scss.js",
    "scss:dist": "npm run scss -- dist",
    "scss:watch": "onchange '**/*.scss' -e 'node_modules/**' -i -- run-s scss",
    "server": "node ./build/server.js",
    "server:gzip": "npm run server -- gzip",
    "start": "run-s build:dist server:gzip",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "run-p build:watch bs"
  },
  "author": "Brandon Buttars <brandonbuttars@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.24.4",
    "copyfile": "^0.1.2",
    "cssnano": "^4.0.0",
    "eslint": "^5.0.1",
    "eslint-plugin-riot": "^0.1.8",
    "http-server": "^0.11.1",
    "localforage": "^1.7.2",
    "md5": "^2.2.1",
    "node-sass": "^4.9.1",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.0.0",
    "postcss": "^6.0.23",
    "postcss-cli": "^5.0.1",
    "postcss-cssnext": "^3.1.0",
    "riot": "^3.10.3",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-riot": "^2.0.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "snarkdown": "^1.2.2",
    "uglify-es": "^3.3.9"
  }
}
